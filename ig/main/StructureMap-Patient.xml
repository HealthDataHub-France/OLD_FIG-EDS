</span>        src<span style="color: navy"><b> -&gt; </b></span>newEntry.resource = <b>create</b><span style="color: navy">(</span><span style="color: blue">'https://health-data-hub.fr/ig/fhir/eds/StructureDefinition/Person'</span><span style="color: navy">)</span><b> as </b><span style="color: maroon">newPerson</span><b> then</b><span style="color: navy"> {
</span>          src<span style="color: navy"><b> -&gt; </b></span>newPerson.person_id = <b>uuid</b><span style="color: navy">(</span><span style="color: navy">)</span><b> as </b><span style="color: maroon">newPersonId</span> <i>&quot;setId&quot;</i><span style="color: navy">;</span>
